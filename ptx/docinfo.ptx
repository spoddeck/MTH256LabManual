<docinfo>
  <macros>
  </macros>

  <latex-image-preamble>
    <![CDATA[
    \usepackage{pgfplots}
    \usetikzlibrary{arrows.meta}
    \pgfplotsset{every axis/.append style={
          axis x line=middle,    % put the x axis in the middle
          axis y line=middle,    % put the y axis in the middle
          axis line style={<->}, % arrows on the axis
          tick label style={/pgf/number format/fixed},
          xlabel={$x$},          % default put x on x-axis
          ylabel={$y$},          % default put y on y-axis
          xmin = -6,xmax = 6,    % most graphs have this window
          ymin = -6,ymax = 6,    % most graphs have this window
          domain = -7:7,
          xtick = {-6,-4,...,6}, % label these ticks
          ytick = {-6,-4,...,6}, % label these ticks
          yticklabel style={inner sep=0.333ex},
          tick label style={font=\footnotesize},
          grid = both
    }}
    \pgfplotsset{function/.style  = {color=blue, mark=none, line width=1pt, -, solid}}
    \pgfplotsset{trigfunction/.style  = {function, {Kite[length=3mm]}-{Kite[length=3mm]}, shorten >=-1.5mm, shorten <=-1.5mm}}
    \pgfplotsset{logfunction/.style  = {function, {Kite[length=3mm]}-{Kite[length=3mm]}, shorten >=-1.5mm, shorten <=-1.5mm}}
    \pgfplotsset{ghost/.style     = {function, color=gray, dotted}}
    \pgfplotsset{soliddot/.style  = {color=blue, mark=*,    only marks}}
    \pgfplotsset{hollowdot/.style = {color=blue, mark=*,    only marks, fill=white}}
    \pgfplotsset{ray/.style       = {color=blue, thick, -{stealth}}}
    \pgfplotsset{angle/.style     = {->, black}}
    \pgfplotsset{guideline/.style = {<->, gray, dashed}}
    ]]>
  </latex-image-preamble>
</docinfo>
