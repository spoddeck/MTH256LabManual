    <!-- OG version example 2 -->
    <example>
      <task>
        <statement>
          <p>
            If <m>f(x)=x^2</m> and <m>g(x)=2x^2+5</m>, express <m>g(x)</m> in terms of <m>f</m>.
          </p>
        </statement>
        <answer>
          <p>
            <m>g(x)=2f(x)+5</m>
          </p>
        </answer>
      </task>
      <task>
        <statement>
          <p>
            If <m>f(x)=x^2</m> and <m>h(x)=(x+5)^2-3</m>, express <m>h(x)</m> in terms of <m>f</m>.
          </p>
        </statement>
        <answer>
          <p>
            <m>h(x)=f(x+5)-3</m>
          </p>
        </answer>
      </task>
    </example>
